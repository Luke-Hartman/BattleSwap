{
    "@title_label": {
        "font": {
            "size": "48"
        }
    },
    "@unit_count": {
        "colours": {
            "normal_bg": "#45494e",
            "hovered_bg": "#35393e",
            "disabled_bg": "#25292e",
            "selected_bg": "#193754",
            "active_bg": "#193754",
            "normal_border": "#DDDDDD",
            "hovered_border": "#FFFFFF",
            "disabled_border": "#808080",
            "selected_border": "#8080B0",
            "active_border": "#8080B0"
        },
        "misc": {
            "border_width": "1",
            "shadow_width": "2",
            "shape": "rectangle"
        }
    },
    "@unit_count_text": {
        "misc": {
            "text_horiz_alignment": "right",
            "text_horiz_alignment_padding": "3"
        }
    },
